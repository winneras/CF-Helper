{
  "manifest_version": 2,

  "name": "CF Helper",
  "description": "Crossfunding Reseash Helper",
  "version": "0.1",
  "icons": {
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "http://www.kickstarter.com/*",
    "https://www.kickstarter.com/*"
  ],
  "content_scripts": [{
    "matches": ["http://www.kickstarter.com/*", "https://www.kickstarter.com/*"],
    "js": ["main.js"]
  }],
  "web_accessible_resources": [
    "cf-helper.js",
    "CFBase.js",
    "KickstarterHandler.js",
    "lib/require.js",
    "lib/moment.min.js"
  ]
}
